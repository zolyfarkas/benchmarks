<?xml version="1.0" encoding="ISO-8859-1"?>
<project name="${project.name}">
    <bannerLeft>
        <name>SPF4J Benchmarks</name>
    </bannerLeft>
    <body>
        <links>
            <item name="SPF4J" href="http://www.spf4j.org"/>
        </links>

        <menu name="Main Menu">
            <item name="Benchmark profiles" href="ssdump/profiles.zip"/>
            <item name="Benchmark results" href="benchmarkResults.csv"/>
